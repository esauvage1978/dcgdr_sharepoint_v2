{% extends 'base.html.twig' %}
{% block page_title %}<i class="fas fa-cog"></i> Administration{% endblock %}
{% block page_content %}

    <div class="row">
        <div class="col-md-2 offset-md-3" >
            {# ... box widget with defaults ... #}
            {% embed 'widgets/card-widget.html.twig'%}

                {% block card_body %}
                    {% for entry in general_entries %}
                        <div class="row ">
                            <div class="col-sm-12">
                                <a href="{{ path(entry.route)}}" target="_self">
                                    <i class="fas fa-angle-right text-purple"></i>  {{ entry.name }}
                                </a>
                            </div>
                        </div>
                    {% endfor %}
                {% endblock %}
            {% endembed %}
        </div>
    </div>
{% endblock %}